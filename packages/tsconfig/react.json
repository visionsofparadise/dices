{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "React",
  "extends": "./base.json",
  "compilerOptions": {
    "lib": [
      "DOM",
      "DOM.AsyncIterable",
      "DOM.Iterable",
      "ESNext",
      "ESNext.AsyncIterable",
    ],
    "jsx": "react-jsx",
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "resolveJsonModule": true
  }
}